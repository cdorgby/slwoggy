{
    "configurations": [
        {
            "name": "Mac",
            "compileCommands": "${workspaceFolder}/build/darwin/compile_commands.json",
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-std=c++20",
                "-stdlib=libc++"
            ],
            "defines": [
                "_LIBCPP_DISABLE_AVAILABILITY"
            ],
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/third_party/fmt/include",
                "${workspaceFolder}/third_party/taocpp-json/include",
                "${workspaceFolder}/third_party/Catch2/src"
            ]
        },
        {
            "name": "Linux",
            "compileCommands": "${workspaceFolder}/build/linux/compile_commands.json",
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-std=c++20"
            ],
            "defines": [],
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/third_party/fmt/include",
                "${workspaceFolder}/third_party/taocpp-json/include",
                "${workspaceFolder}/third_party/Catch2/src"
            ]
        },
        {
            "name": "Win32",
            "compileCommands": "${workspaceFolder}/build/windows/compile_commands.json",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-msvc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/third_party/fmt/include",
                "${workspaceFolder}/third_party/taocpp-json/include",
                "${workspaceFolder}/third_party/Catch2/src"
            ],
            "windowsSdkVersion": "10.0.19041.0"
        }
    ],
    "version": 4
}